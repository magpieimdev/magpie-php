{
    "name": "magpieim/magpie-php",
    "description": "The Magpie API Library for PHP enables you to work with Magpie APIs. Built for Laravel and modern PHP applications.",
    "keywords": [
        "magpie",
        "payment processing",
        "credit cards",
        "api",
        "php",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Magpie Team",
            "email": "support@magpie.im",
            "homepage": "https://magpie.im/"
        },
        {
            "name": "Jerick Coneras",
            "email": "jerick@magpie.im"
        }
    ],
    "homepage": "https://github.com/magpieimdev/magpie-php",
    "support": {
        "email": "support@magpie.im",
        "docs": "https://docs.magpie.im",
        "issues": "https://github.com/magpieimdev/magpie-php/issues",
        "source": "https://github.com/magpieimdev/magpie-php"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0|^2.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5",
        "php-cs-fixer/shim": "^3.0",
        "phpstan/phpstan": "^1.0",
        "orchestra/testbench": "^8.0",
        "laravel/framework": "^10.0"
    },
    "suggest": {
        "laravel/framework": "Required for Laravel integration features"
    },
    "autoload": {
        "psr-4": {
            "Magpie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Magpie\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "fix": "php-cs-fixer fix",
        "analyse": "phpstan analyse src",
        "check": [
            "@analyse",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Magpie\\Laravel\\MagpieServiceProvider"
            ],
            "aliases": {
                "Magpie": "Magpie\\Laravel\\Facades\\Magpie"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
